<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<!-- 컬럼 이름이 언더바로 연결되어 있을 경우 카멜케이스로 변경해준다. -->
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.oti.thirtyone.dto.EmployeesDto" alias="Employee"/>
		<typeAlias type="com.oti.thirtyone.dto.NoticeDto" alias="Notice"/>
		<typeAlias type="com.oti.thirtyone.dto.NoticeFileDto" alias="NoticeFile"/>
		<typeAlias type="com.oti.thirtyone.dto.Departments" alias="Department"/>
		<typeAlias type="com.oti.thirtyone.dto.PositionsDto" alias="Position"/>
		
		<typeAlias type="com.oti.thirtyone.dto.Pager" alias="Pager"/>	
		
	</typeAliases>
</configuration>